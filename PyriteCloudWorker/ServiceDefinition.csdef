<?xml version="1.0" encoding="utf-8"?>
<ServiceDefinition name="PyriteCloudWorker" xmlns="http://schemas.microsoft.com/ServiceHosting/2008/10/ServiceDefinition" schemaVersion="2015-04.2.6">
  <WorkerRole name="PyriteCloudRole" vmsize="A9">
    <ConfigurationSettings>
      <Setting name="Microsoft.WindowsAzure.Plugins.Diagnostics.ConnectionString" />
      <Setting name="StorageConnectionString" />
      <Setting name="Queue" />
    </ConfigurationSettings>
    <LocalResources>
      <LocalStorage name="input" cleanOnRoleRecycle="true" sizeInMB="5000" />
      <LocalStorage name="output" cleanOnRoleRecycle="true" sizeInMB="10000" />
    </LocalResources>
  </WorkerRole>
</ServiceDefinition>