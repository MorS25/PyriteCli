<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
    <system.diagnostics>
      <trace autoflush="true" indentsize="4">
        <listeners>
          <add name="textLog" type="System.Diagnostics.TextWriterTraceListener" initializeData="cuber.log" />
          <add name="consoleListener" type="System.Diagnostics.ConsoleTraceListener" />
          <remove name="Default" />
        </listeners>
      </trace>
    </system.diagnostics>
</configuration>